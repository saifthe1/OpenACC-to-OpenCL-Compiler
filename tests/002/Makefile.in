          -I@top_srcdir@/libopenacc/include \
	  -I@opencl_inc_path@ \
          -DLIBOPENACC_DIR=\"@top_srcdir@/libopenacc/lib\" \
          -DKERNEL_DIR=\"@srcdir@\" \
